{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript"
    ],
    "overrides": [
    ],
    "plugins": [
        "react"
    ],
    "rules": {
        "@typescript-eslint/semi": "off",
        "@typescript-eslint/promise-function-async": "off"
    },
    "parserOptions": {
        "project": ["tsconfig.json"],
        "ecmaVersion": "latest",
        "sourceType": "module"
    }

}
